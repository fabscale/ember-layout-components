{
  "name": "ember-layout-components",
  "version": "2.0.0",
  "description": "Generic & reusable layout components for your Ember app.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/fabscale/ember-layout-components",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "run-prettier": "prettier --write \"**/*.{js,scss}\"",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:ember-one": "ember try:one",
    "release": "release-it"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "~7.18.2",
    "@babel/eslint-parser": "~7.18.2",
    "@ember/optional-features": "~2.0.0",
    "@ember/test-helpers": "~2.8.1",
    "@embroider/test-setup": "~1.8.0",
    "@glimmer/component": "~1.1.2",
    "@glimmer/tracking": "~1.1.2",
    "broccoli-asset-rev": "~3.0.0",
    "ember-auto-import": "~2.4.2",
    "ember-cli": "~4.5.0",
    "ember-cli-dependency-checker": "~3.3.1",
    "ember-cli-inject-live-reload": "~2.1.0",
    "ember-cli-sri": "~2.1.1",
    "ember-cli-terser": "~4.0.2",
    "ember-disable-prototype-extensions": "~1.1.3",
    "ember-load-initializers": "~2.1.2",
    "ember-page-title": "~7.0.0",
    "ember-prism": "~0.12.0",
    "ember-qunit": "~5.1.5",
    "ember-resolver": "~8.0.3",
    "ember-source": "~4.5.0",
    "ember-source-channel-url": "~3.0.0",
    "ember-template-lint": "~4.10.0",
    "ember-try": "~2.0.0",
    "eslint": "~8.19.0",
    "fabscale-eslint-config": "~1.5.0",
    "loader.js": "~4.7.0",
    "npm-run-all": "~4.1.5",
    "prettier": "~2.7.1",
    "qunit": "~2.19.1",
    "qunit-dom": "~2.0.0",
    "release-it": "~14.14.0",
    "release-it-lerna-changelog": "~4.0.1",
    "webpack": "~5.73.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "volta": {
    "node": "16.13.0",
    "yarn": "1.22.17"
  },
  "resolutions": {
    "*/ember-auto-import": "~2.4.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://fabscale.github.io/ember-layout-components/"
  }
}
