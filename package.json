{
  "name": "ember-layout-components",
  "version": "1.3.0",
  "description": "Generic & reusable layout components for your Ember app.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/fabscale/ember-layout-components",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "run-prettier": "prettier --write \"**/*.{js,scss}\"",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:ember-one": "ember try:one",
    "release": "release-it"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^6.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "~2.0.0",
    "@ember/test-helpers": "~2.4.2",
    "@embroider/test-setup": "~0.47.1",
    "@glimmer/component": "~1.0.4",
    "@glimmer/tracking": "~1.0.4",
    "babel-eslint": "~10.1.0",
    "broccoli-asset-rev": "~3.0.0",
    "ember-auto-import": "~2.2.0",
    "ember-cli": "~3.28.0",
    "ember-cli-dependency-checker": "~3.2.0",
    "ember-cli-inject-live-reload": "~2.1.0",
    "ember-cli-sri": "~2.1.1",
    "ember-cli-terser": "~4.0.2",
    "ember-disable-prototype-extensions": "~1.1.3",
    "ember-export-application-global": "~2.0.1",
    "ember-load-initializers": "~2.1.2",
    "ember-maybe-import-regenerator": "~1.0.0",
    "ember-page-title": "~6.2.2",
    "ember-prism": "~0.12.0",
    "ember-qunit": "~5.1.4",
    "ember-resolver": "~8.0.2",
    "ember-source": "~3.28.0",
    "ember-source-channel-url": "~3.0.0",
    "ember-template-lint": "~3.8.0",
    "ember-try": "~1.4.0",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-ember": "~10.5.4",
    "eslint-plugin-ember-es6-class": "~0.0.1",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "~4.7.0",
    "npm-run-all": "~4.1.5",
    "prettier": "~2.4.1",
    "qunit": "~2.17.2",
    "qunit-dom": "~2.0.0",
    "release-it": "~14.11.5",
    "release-it-lerna-changelog": "~3.1.0",
    "webpack": "~5.56.1"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "volta": {
    "node": "16.13.0",
    "yarn": "1.22.17"
  },
  "resolutions": {
    "*/ember-auto-import": "~2.2.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://fabscale.github.io/ember-layout-components/"
  }
}