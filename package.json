{
  "name": "ember-layout-components",
  "version": "0.5.2",
  "description": "Generic & reusable layout components for your Ember app.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/fabscale/ember-layout-components",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "run-prettier": "prettier --write \"**/*.{js,scss}\"",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:ember-one": "ember try:one",
    "release": "release-it"
  },
  "dependencies": {
    "ember-cli-babel": "^7.19.0",
    "ember-cli-htmlbars": "^5.1.2"
  },
  "devDependencies": {
    "@ember/optional-features": "~1.3.0",
    "@glimmer/component": "~1.0.0",
    "@glimmer/tracking": "~1.0.0",
    "@release-it/conventional-changelog": "~1.1.0",
    "babel-eslint": "~10.1.0",
    "broccoli-asset-rev": "~3.0.0",
    "ember-auto-import": "~1.5.3",
    "ember-cli": "~3.18.0",
    "ember-cli-dependency-checker": "~3.2.0",
    "ember-cli-inject-live-reload": "~2.0.2",
    "ember-cli-sri": "~2.1.1",
    "ember-cli-uglify": "~3.0.0",
    "ember-disable-prototype-extensions": "~1.1.3",
    "ember-export-application-global": "~2.0.1",
    "ember-load-initializers": "~2.1.1",
    "ember-maybe-import-regenerator": "~0.1.6",
    "ember-prism": "^0.7.0",
    "ember-qunit": "~4.6.0",
    "ember-resolver": "~8.0.0",
    "ember-source": "~3.19.0",
    "ember-source-channel-url": "~2.0.1",
    "ember-template-lint": "~2.8.0",
    "ember-try": "~1.4.0",
    "eslint": "~7.3.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-ember": "~8.7.0",
    "eslint-plugin-ember-es6-class": "~0.0.1",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~3.1.2",
    "loader.js": "~4.7.0",
    "npm-run-all": "~4.1.5",
    "prettier": "~2.0.1",
    "qunit-dom": "~1.2.0",
    "release-it": "~13.6.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "volta": {
    "node": "12.17.0",
    "yarn": "1.22.4"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://fabscale.github.io/ember-layout-components/"
  }
}
